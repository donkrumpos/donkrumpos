#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/donkrumpos/Documents/active_projects_local/donkrumpos/node_modules/svgo/bin/node_modules:/Users/donkrumpos/Documents/active_projects_local/donkrumpos/node_modules/svgo/node_modules:/Users/donkrumpos/Documents/active_projects_local/donkrumpos/node_modules:/Users/donkrumpos/Documents/active_projects_local/node_modules:/Users/donkrumpos/Documents/node_modules:/Users/donkrumpos/node_modules:/Users/node_modules:/node_modules:/Users/donkrumpos/Documents/active_projects_local/donkrumpos/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/donkrumpos/Documents/active_projects_local/donkrumpos/node_modules/svgo/bin/node_modules:/Users/donkrumpos/Documents/active_projects_local/donkrumpos/node_modules/svgo/node_modules:/Users/donkrumpos/Documents/active_projects_local/donkrumpos/node_modules:/Users/donkrumpos/Documents/active_projects_local/node_modules:/Users/donkrumpos/Documents/node_modules:/Users/donkrumpos/node_modules:/Users/node_modules:/node_modules:/Users/donkrumpos/Documents/active_projects_local/donkrumpos/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../svgo/bin/svgo.js" "$@"
else
  exec node  "$basedir/../svgo/bin/svgo.js" "$@"
fi
